Default setting, which is the same setting in the paper

# nature dqn
python main.py --arch=DQN --eps=1.0 --final_eps=0.1 --max_frames=6000000 --clip=False --fast_test=False --opt=rmsprop --lr=0.0025

# C51
python main.py --arch=C51 --eps=1.0 --final_eps=0.05 --max_frames=6000000 --clip=False --fast_test=False --opt=adam --lr=0.00025

# QR_DQN
python main.py --arch=QR_DQN --eps=1.0 --final_eps=0.01 --max_frames=6000000 --clip=False --fast_test=False --opt=adam --lr=0.000005

